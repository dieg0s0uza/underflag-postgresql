{
  "name": "underflag-postgresql",
  "version": "1.0.1",
  "description": "postgresql provider for underflag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "prepublish": "npm run test && npm run build",
    "example": "npx ts-node examples/postgresql",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieg0s0uza/underflag-postgresql.git"
  },
  "keywords": [
    "feature flag",
    "feature toggle",
    "remote config",
    "postgresql"
  ],
  "author": "Diego Souza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dieg0s0uza/underflag-postgresql/issues"
  },
  "homepage": "https://github.com/dieg0s0uza/underflag-postgresql#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.8",
    "@types/pg": "^8.6.3",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "pg": "^8.7.1",
    "underflag": "^1.3.0"
  }
}